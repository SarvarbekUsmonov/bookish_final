ish_react/src/Home.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/Login.js",["93"],[],"import React from 'react'\nimport { useRef } from 'react';\n\n\nfunction Login(){\n    const usernameRef = useRef('null');\n    const passwordRef = useRef('null');\n    async function handleLoginAccount(){\n        const username = usernameRef.current.value;\n        const password = passwordRef.current.value;\n        const response = await fetch('http://localhost:4000/login', {\n            method: 'POST',\n            credentials: 'include',\n            headers: {\n                'Content-Type': 'application/json',\n                \n            },\n            body: JSON.stringify({\n                username: username,\n                password: password\n            })\n        })\n        const data = await response\n        if(data.status == 200){\n            window.location.href = \"/\"\n        }else{\n            alert('Username or Password is incorrect')\n        }\n    }\n    return(\n        // create a form for the user to login\n        <div id='loginPage'>\n            <div id='loginText'>Login</div>\n            <form id=\"loginForm\">\n                <label for=\"username\">Username</label>\n                <input ref={usernameRef} type=\"text\" name=\"username\" id=\"loginUsername\" placeholder=\"Username\" required></input>\n                <label for=\"password\">Password</label>\n                <input ref={passwordRef} type=\"password\" name=\"password\" id=\"loginPassword\" placeholder=\"Enter Password Here\" required></input>\n                <button id=\"loginBtn\" type=\"button\" onClick={handleLoginAccount}>Log in</button>\n                <a href=\"./signUp\" id='signupLink'>Don't have an account? Sign up here.</a>\n            </form>\n            <div id=\"copyrightDate\">2023</div>\n        </div>\n    )\n}\n\nexport default Login;","/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/Search.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/View.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/Post.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/SecondPageHome.js",["94","95"],[],"import React, { useState, useEffect, Component } from \"react\";\nimport { Link } from 'react-router-dom';\n\nfunction SecondPageHome(){\n  const [bookList, setBookList] = useState([]);\n\n  useEffect(() => {\n    function fetchRecommendedBooks() {\n      fetch('http://localhost:4000/recommended/')\n        .then((response) => response.json())\n        .then((data) => setBookList(data))\n        .catch((error) => console.log(error));\n    }\n\n    fetchRecommendedBooks();\n  }, []);\n\n  // Divide the bookList into chunks of three\n  const chunks = bookList.reduce((acc, book, index) => {\n    const chunkIndex = Math.floor(index / 3);\n    if (!acc[chunkIndex]) {\n      acc[chunkIndex] = [];\n    }\n    acc[chunkIndex].push(book);\n    return acc;\n  }, []);\n\n  return(\n    <div id=\"secondary\">\n      <h2 id=\"recommended-text\">Recommended:</h2>\n      <div id=\"recommended-books\" className=\"container\">\n        {chunks.map((chunk, index) => (\n          <div key={index} className=\"row\">\n            {chunk.map((book) => (\n              <div className=\"column card mt-4 mx-auto\" style={{width: \"18rem\"}} key={book.id}>\n                <img src={require('../images/Game_of_thrones.webp')} className=\"card-img-top\" alt=\"book cover\"></img>\n                <div className=\"card-body\">\n                  <h5 className=\"card-title\">{book.title}</h5>\n                  <p className=\"card-text\">{book.description}</p>\n                  <a href={`/view?BookID=${book._id}#s`}>View component</a>\n                </div>\n              </div>\n            ))}\n          </div>\n        ))}\n      </div>\n    </div>\n  )\n}\n\nexport default SecondPageHome;\n","/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/ViewBook.js",["96","97"],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/MainHome.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/Filter.js",["98"],[],"import React, { useState, useEffect, useRef } from \"react\";\nimport { useLocation } from \"react-router-dom\";\n\nfunction Filter({ updateBooks }) {\n  const [authors, setAuthors] = useState([]);\n\n  useEffect(() => {\n    fetch(\"http://localhost:4000/getAuthors\")\n      .then((response) => response.json())\n      .then((data) => {\n        setAuthors(data);\n      });\n  }, []);\n\n  const authorRef = useRef(null);\n  const genreRef = useRef(null);\n  const yearRef = useRef(null);\n  const inputRef = useRef(null);\n  const location = useLocation();\n  const qP = new URLSearchParams(location.search);\n  const input = qP.get('input');\n\n  function filter() {\n    let author = authorRef.current.value;\n    let genre = genreRef.current.value;\n    let year = yearRef.current.value;\n    if (author === \"Author\") {\n      author = false;\n    }\n    if (genre === \"Genre\") {\n      genre = false;\n    }\n    if (year === \"Year\") {\n      year = false;\n    }\n    const url = `http://localhost:4000/filter/${author}/${year}/${genre}/${inputRef.current.value}`;\n    console.log(url)\n    fetch(url)\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data);\n        updateBooks(data);\n      });\n  }\n\n  function filter2() {\n    const url = `http://localhost:4000/filter/${false}/${false}/${false}/${input}`;\n    console.log(url)\n    fetch(url)\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data);\n        updateBooks(data);\n      });\n  }\n\n  // Call filter2() only once when the component is mounted\n  useEffect(() => {\n    filter2();\n  }, []);\n\n   \n  return (\n    <div id=\"filter\">\n      <form id=\"searchForm\" onSubmit={(e) => { e.preventDefault(); filter(); }}>\n        <div className=\"form-group d-flex\">\n          <input ref={inputRef} id=\"search-bar\" className=\"form-control  me-2 mx-2\" type=\"search\" placeholder=\"Search\" aria-label=\"Search\" required></input>\n          <button id=\"search-button\" className=\"btn btn-outline-success mx-2\" type=\"submit\">Search</button>\n        </div>\n        <div className=\"form-group d-flex my-4 mx-2 align-items-center\">\n          <label className=\"form-label\">Genre:</label>\n          <select ref={genreRef} className=\"form-select w-50 \" aria-label=\"Default select example\">\n          <option value=\"\">Select a genre</option>\n          <option value=\"Fantasy\">Fantasy</option>\n          <option value=\"Romance\">Romance</option>\n          <option value=\"Mystery\">Mystery</option>\n          <option value=\"Science Fiction\">Science Fiction</option>\n          <option value=\"Horror\">Horror</option>\n          <option value=\"Thriller\">Thriller</option>\n          <option value=\"Historical Fiction\">Historical Fiction</option>\n          <option value=\"Nonfiction\">Nonfiction</option>\n          <option value=\"Young Adult\">Young Adult</option>\n          <option value=\"Biography\">Biography</option>\n          <option value=\"Comedy\">Comedy</option>\n          <option value=\"Drama\">Drama</option>\n          <option value=\"Action and Adventure\">Action and Adventure</option>\n          <option value=\"Crime and Detective\">Crime and Detective</option>\n          </select>\n        </div>\n        <div className=\"form-group d-flex my-4 mx-2 align-items-center\">\n          <label className=\"form-label\">Author:</label>\n          <select ref={authorRef}  className=\"form-select w-50 \" aria-label=\"Default select example\">\n            <option selected>Author</option>\n            {authors.map((author) => (\n              <option key={author} value={author}>\n                {author}\n              </option>\n            ))}\n          </select>\n        </div>\n        <div className=\"form-group d-flex my-4 mx-2 align-items-center\">\n          <label className=\"form-label\">Year:</label>\n          <select ref={yearRef}  className=\"form-select w-50\" aria-label=\"Year\" menuPosition=\"fixed\">\n            <option>Year</option>\n            {Array.from({ length: new Date().getFullYear() - 1900 }, (_, index) => (\n              <option key={index}>{new Date().getFullYear() - index}</option>\n            ))}\n          </select>\n        </div>\n      </form>\n    </div>\n  );\n}\n\nexport default Filter\n","/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/ContentSearch.js",["99","100","101","102","103","104"],[],"import React, { useEffect, useState } from \"react\";\nimport { useLocation } from \"react-router-dom\";\n\nfunction ContentSearch({books}){\n\n  console.log(books)\n  return(\n    <div id=\"content\">\n      <nav class=\"navbar navbar-expand-md sticky-top bg-white border-bottom m-0 py-0\">\n        <img class=\"rounded d-block mx-2\" id=\"logo-img\" src={require(\"../images/photo_2023-04-13 23.51.37.jpeg\")}></img>\n        <a class=\"navbar-brand me-auto\">Bookish</a>\n        <img id=\"pfp-img\" src=\"https://www.shareicon.net/data/512x512/2016/02/22/722964_button_512x512.png\" class=\"rounded mx-4 d-block\" alt=\"user image\"></img>\n        \n      </nav>\n      {books.map((book, index)=>(\n        <div class=\"card mb-3 mx-auto my-5\" style={{maxWidth: \"540px\"}}>\n        <div class=\"row g-0\">\n          <div class=\"col-md-4\">\n            <img src={require(\"../images/Game_of_thrones.webp\")} class=\"img-fluid rounded-start\" alt=\"...\"></img>\n          </div>\n          <div class=\"col-md-8\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">{book.title}</h5>\n              <p class=\"card-text\">{book.description}</p>\n              <p class=\"card-text\"><small class=\"text-body-secondary\">{book.author}</small></p>\n            </div>\n          </div>\n        </div>\n      </div>\n      ))}\n\n    </div>\n  )\n}\n\nexport default ContentSearch;","/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/Navbar.js",["105"],[],"import React, { useRef, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\n\nfunction Navbar(){\n  const navBarReact = useRef(null); // create a ref using useRef hook\n  const [searchValue, setSearchValue] = useState(\"\");\n\n  function handleSearchInputChange() {\n    setSearchValue(navBarReact.current.value);\n  }\n\n  return (\n    <div>\n      <nav className=\"navbar navbar-expand-md sticky-top bg-white m-0 py-0\">\n        <img\n          className=\"rounded d-block mx-4\"\n          alt=\"\"\n          id=\"logo-img\"\n          src={require(\"../images/photo_2023-04-13 23.51.37.jpeg\")}\n        />\n        <a className=\"navbar-brand\">Bookish</a>\n        <form className=\"d-flex mx-auto search\" role=\"search\" id=\"searchFormNavbar\">\n          <input ref={navBarReact}\n            id=\"search-bar\"\n            className=\"form-control  me-2 mx-2\"\n            type=\"search\"\n            placeholder=\"Search\"\n            aria-label=\"Search\"\n            onChange={handleSearchInputChange}\n          ></input>\n          <Link\n            id=\"search-button\"\n            className=\"btn btn-outline-success mx-auto\"\n            to={`/search?input=${searchValue}`}\n          >\n            Search\n          </Link>\n        </form>\n        <img id=\"pfp-img\" className=\"rounded mx-4 d-block\" alt=\"\"></img>\n      </nav>\n    </div>\n  );\n}\n\nexport default Navbar;\n","/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/Rateandcomment.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/components/Bookcomments.js",[],[],"/Users/nurkhatjumabaev/Desktop/bookish_final/bookish_react/src/ChangeAvatar.js",[],[],"/Users/n